
public class Ex01 {

	public static void main(String[] args) {
		/*
		 * ++A : 선위 연산자 (증감)
		 * B++ : 후위 연산자
		 * --A : 감소
		 * B-- :  
		 * */
int iNum = 10;

System.out.println(iNum);

System.out.println("iNum 1증가 - 1");

iNum = iNum + 1;

 System.out.println(iNum);
 
 iNum += 1;
 
 System.out.println("iNum 1증가 - 2");
 iNum += 1;
 System.out.println("iNum 1증가 - 3");
 ++iNum; 
	}

}
